# MOONLIT Lab Portal Automation Environment Variables
# Copy this file to .env and fill in your actual credentials

# Portal Credentials
LABCORP_USERNAME=your_labcorp_username
LABCORP_PASSWORD=your_labcorp_password
QUEST_USERNAME=your_quest_username
QUEST_PASSWORD=your_quest_password

# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_KEY=your_supabase_service_key

# Google Gemini Configuration (HIPAA-compliant LLM for adaptive form filling)
GEMINI_API_KEY=your_gemini_api_key

# Server Configuration
PORT=3001
NODE_ENV=development

# Automation Settings
HEADLESS_MODE=false
SCREENSHOT_PATH=./test-screenshots
MAX_RETRY_ATTEMPTS=3
AUTOMATION_DELAY_MS=1000

# Session Management
SESSION_SECRET=your_session_secret_here
JWT_SECRET=your_jwt_secret_here

# Preview Mode
ENABLE_PREVIEW_MODE=true
AUTO_SUBMIT_TIMEOUT_MS=0

# Notification Settings
ALERT_EMAIL=admin@moonlit.com
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=notifications@moonlit.com
SMTP_PASS=your_email_password